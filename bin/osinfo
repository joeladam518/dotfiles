#!/usr/bin/env bash

SCRIPT_DIR="$(cd "$(dirname "$0")" > /dev/null 2>&1 && pwd -P)"
PYHTON_DIR="$(dirname "$SCRIPT_DIR")/python"

if [ -n "$PYTHONPATH" ]; then
    export PYTHONPATH="${PYTHONPATH}:${PYHTON_DIR}"
else
    export PYTHONPATH="${PYHTON_DIR}"    
fi

python3 "${PYHTON_DIR}/dotfiles/osinfo.py" "$@"
